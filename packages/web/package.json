{
  "name": "@kyc/web",
  "version": "1.0.0",
  "description": "Frontend em Next.js para o sistema KYC",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run tailwind:watch\" \"next dev\"",
    "build": "tailwindcss -i ./src/app/globals.css -o ./public/css/tailwind.css && next build",
    "start": "next start",
    "lint": "next lint",
    "tailwind:build": "tailwindcss -i ./src/app/globals.css -o ./public/css/tailwind.css",
    "tailwind:watch": "tailwindcss -i ./src/app/globals.css -o ./public/css/tailwind.css --watch"
  },
  "keywords": [
    "kyc",
    "frontend",
    "next.js"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.8.0",
  "dependencies": {
    "next": "^15.3.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/react": "19.1.3",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.3.0"
  }
}
